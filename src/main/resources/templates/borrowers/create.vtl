<h1>Create New Borrower</h1>


<form action="/borrowers" method="post">
    <label for="firstName">First Name: </label>
    <input type="text" class="inputBox" name="firstName" required="true" id="firstName"/>

    <label for="lastName">Last Name: </label>
    <input type="text" class="inputBox" name="lastName" required="true" id="lastName"/>

    <input type="submit" value="save" class="button"/>
</form>