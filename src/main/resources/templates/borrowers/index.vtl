<p class="pageHeader"> All Borrowers</p>

<div>
    <a href="/borrowers/new" class="button center">Add New Borrower</a>
</div>

<div class="container">
    <table>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
        </tr>
        #foreach($borrower in $borrowers)
        <tr>
            <td>$borrower.getFirstName()</td>
            <td>$borrower.getLastName()</td>
        </tr>
        #end
    </table>

</div>